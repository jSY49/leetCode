*/

class Solution {
    fun removeElement(nums: IntArray, `val`: Int): Int {

        var res =0
        for(i in 0..nums.size-1){
             if (nums[i] != `val`) {
                nums[res++] = nums[i]
            }
        }

        return res
    }
}
